{
    "version": 2,
    "builds": [
        {
            "src": "dist/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**",
                    "package.json",
                    "favicon.ico",
                    "public/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/index.js"
        }
    ]
}