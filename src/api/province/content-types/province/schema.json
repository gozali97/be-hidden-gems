{
    "kind": "collectionType",
    "collectionName": "provinces",
    "info": {
        "singularName": "province",
        "pluralName": "provinces",
        "displayName": "Province",
        "description": "Indonesian provinces grouped by region"
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "name": {
            "type": "string",
            "required": true,
            "unique": true
        },
        "slug": {
            "type": "uid",
            "targetField": "name",
            "required": true
        },
        "region": {
            "type": "enumeration",
            "enum": [
                "Sumatra",
                "Java",
                "Kalimantan",
                "Sulawesi",
                "Papua",
                "Bali & Nusa Tenggara"
            ],
            "required": true
        }
    }
}